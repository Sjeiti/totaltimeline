{
  "name": "totaltimeline",
  "description": "Total Timeline",
  "author": "Ron Valstar",
  "version": "0.1.73",
  "repository": "https://xp-dev.com/git/totaltimeline",
  "scripts": {
    "start": "run-p build serve",
    "build": "run-s clean \"webpack -- --env.development --watch\"",
    "build:prod": "run-s clean icomoon \"webpack -- --env.production\" build:sitemap",
    "build:sitemap": "node task/sitemap",
    "webpack": "npx webpack --config webpack.config.js",
    "lint": "eslint src/js/index.js",
    "serve": "node ./task/serve dist 8385",
    "json": "node ./task/wiki ./src/static/events.json",
    "clean": "rimraf dist",
    "icomoon": "node task/icomoon",
    "icons": "node task/img2css --source ./src/icon/ --target ./src/style/icons.less",
    "version": "next-version package.json src/serviceWorker.js"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "adm-zip": "^0.4.11",
    "autoprefixer": "^7.1.2",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.6",
    "babel-preset-babili": "0.0.12",
    "babel-preset-minify": "^0.4.3",
    "body-parser": "^1.17.2",
    "chokidar-cli": "^1.2.0",
    "commander": "^2.11.0",
    "copy": "^0.3.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "cssnano": "^3.10.0",
    "eslint": "^4.18.2",
    "eslint-plugin-html": "^3.1.1",
    "express": "^4.15.3",
    "file-loader": "^4.2.0",
    "less": "^2.7.2",
    "less-loader": "^5.0.0",
    "less-plugin-glob": "^3.0.0",
    "mkdirp": "^0.5.1",
    "next-version": "^0.11.7",
    "npm-run-all": "^4.1.5",
    "open": "6.0.0",
    "postcss-cli": "^4.1.0",
    "raw-loader": "^3.1.0",
    "rimraf": "^2.6.1",
    "serve-static": "^1.12.3",
    "shx": "^0.2.2",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-watch-files-plugin": "^1.0.3",
    "xml2js": "^0.4.19",
    "xmlbuilder": "^9.0.4",
    "yorkie": "^2.0.0"
