{
  "name": "totaltimeline",
  "description": "Total Timeline",
  "version": "0.1.62",
  "repository": "https://xp-dev.com/git/totaltimeline",
  "scripts": {
    "build": "npm run clean && npm run dir && npm run js && npm run less && npm run copy",
    "copy": "shx cp src/index.html dist/index.html | shx cp src/.htaccess dist/.htaccess | shx cp -r src/static dist | shx cp src/style/*.png dist/style",
    "lint": "eslint src/js/index.js",
    "js": "webpack --config webpack.config.js",
    "js:watch": "chokidar src/**/*.js -c \"npm run js\"",
    "serve": "node ./task/serve dist 8385",
    "less": "lessc src/style/screen.less dist/style/screen.css",
    "dir": "mkdirp dist/js",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-babili": "0.0.12",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.17.2",
    "chokidar-cli": "^1.2.0",
    "eslint": "^4.4.1",
    "eslint-plugin-html": "^3.1.1",
    "express": "^4.15.3",
    "less": "^2.7.2",
    "mkdirp": "^0.5.1",
    "open": "0.0.5",
    "rimraf": "^2.6.1",
    "serve-static": "^1.12.3",
    "shx": "^0.2.2",
    "webpack": "^3.4.1"
  },
  "_devDependencies": {
    "glob": "~3.2.7",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "~0.5.0",
